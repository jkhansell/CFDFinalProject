FoamFile
{
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (1.1516 0 0);

boundaryField
{
    sides
    {
        type            empty;
    }
    inlet
    {
        type            fixedValue;
        value           uniform (1.1516 0 0);
    }
    outlet
    {
        type            zeroGradient;
    }
    airfoil
    {
        type            noSlip;
    }
    procBoundary1to0
    {
        type            processor;
        value           uniform (1.1516 0 0);
    }
    procBoundary1to2
    {
        type            processor;
        value           uniform (1.1516 0 0);
    }
}


